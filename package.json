{
    "name": "hl-ui",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "sh -c 'cp .env.development .env && NEXT_PUBLIC_COMMIT_TIMESTAMP=$(date +%s) next dev --turbo'",
        "dev:no-turbo": "sh -c 'cp .env.development .env && NEXT_PUBLIC_COMMIT_TIMESTAMP=$(date +%s) next dev'",
        "dev:prod": "sh -c 'cp .env.production .env && NEXT_PUBLIC_COMMIT_TIMESTAMP=$(date +%s) next dev --turbo'",
        "build": "export NEXT_PUBLIC_COMMIT_TIMESTAMP=$(git log -1 --format=%ct) && next build",
        "build:analyze": "ANALYZE=true export NEXT_PUBLIC_COMMIT_TIMESTAMP=$(git log -1 --format=%ct) && next build -c next.config.analyzer.js",
        "start": "next start",
        "lint": "next lint",
        "eslint:fix": "eslint ./src --fix --ignore-pattern 'src/generated/**'",
        "prettier:fix": "prettier --write src",
        "lint:fix": "pnpm eslint:fix && pnpm prettier:fix",
        "vercel-build": "export NEXT_PUBLIC_COMMIT_TIMESTAMP=$(git log -1 --format=%ct) && next build"
    },
    "dependencies": {
        "@heroui/slider": "^2.4.22",
        "@heroui/tooltip": "^2.2.21",
        "@nktkas/hyperliquid": "^0.24.3",
        "@privy-io/react-auth": "^2.24.0",
        "@privy-io/wagmi": "^1.0.6",
        "@t3-oss/env-nextjs": "^0.13.8",
        "@tanstack/react-query": "^5.86.0",
        "@tanstack/react-virtual": "^3.13.12",
        "@vercel/analytics": "^1.5.0",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.18",
        "echarts": "^6.0.0",
        "ethers": "^6.15.0",
        "framer-motion": "^11.18.2",
        "lightweight-charts": "^5.0.8",
        "lucide-react": "^0.542.0",
        "next": "15.2.1",
        "next-themes": "^0.4.6",
        "numeral": "^2.0.6",
        "nuqs": "^2.4.3",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-error-boundary": "^4.1.2",
        "react-hot-toast": "^2.5.2",
        "tailwind-merge": "^2.6.0",
        "viem": "^2.37.3",
        "wagmi": "^2.16.9",
        "zod": "^4.0.15",
        "zustand": "^4.5.7"
    },
    "devDependencies": {
        "@iconify/react": "^5.2.1",
        "@next/bundle-analyzer": "^15.4.6",
        "@next/eslint-plugin-next": "15.2.1",
        "@types/echarts": "^5.0.0",
        "@types/node": "^20.19.9",
        "@types/numeral": "^2.0.5",
        "@types/react": "19.0.10",
        "@types/react-dom": "19.0.4",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "concurrently": "^9.2.0",
        "eslint": "^9.32.0",
        "eslint-config-next": "15.2.1",
        "eslint-config-prettier": "^9.1.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-tailwindcss": "^3.18.2",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^3.4.17",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2"
    },
    "optionalDependencies": {
        "sharp": "^0.34.3"
    },
    "pnpm": {
        "overrides": {
            "@types/react": "19.0.10",
            "@types/react-dom": "19.0.4",
            "use-sync-external-store": "^1.2.2"
        }
    }
}
